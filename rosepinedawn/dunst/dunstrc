# ~/.config/dunst/dunstrc

[global]
font = Iosevka Nerd Font 14
format = "<b>%s</b>\n%b"
icon_position = left

[urgency_low]
    # IMPORTANT: colors have to be defined in quotation marks.
    # Otherwise the "#" and following would be interpreted as a comment.
    background = "#f2e9de"
    foreground = "#57527b"
    frame_color = "#d7827e"
    timeout = 10
    # Icon for notifications with low urgency, uncomment to enable
    #default_icon = /path/to/icon

[urgency_normal]
    background = "#f2e9de"
    foreground = "#57527b"
    highlight = "#d7827e"
    timeout = 10
    # Icon for notifications with normal urgency, uncomment to enable
    #default_icon = /path/to/icon

[urgency_critical]
    background = "#f2e9de"
    foreground = "#57527b"
    frame_color = "#d7827e"
    timeout = 0

[brightness]
  summary = "ï†… Light"
  history_ignore = yes
  set_stack_tag = "light"
